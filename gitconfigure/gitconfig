[alias]
  s = status
  stats = status
  ci = commit
  br = branch
  co = checkout
  d = diff
  lg = log -p
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[color]
  ui = true
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = ~/.gitignore
[apply]
  whitespace = strip
[push]
  default = simple
[pull]
  rebase = true
[merge]
  conflictstyle = diff3
[fetch]
  prune = true
[diff]
  algorithm = patience
[protocol]
  version = 2
